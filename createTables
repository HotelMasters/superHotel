CREATE TABLE "GUEST" (
    "ID" BIGINT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
    "NAME" VARCHAR(255) NOT NULL,
    "ADDRESS" VARCHAR (255) NOT NULL,
    "BIRTHDAY" DATE,
    "CR.CARD NUMBER" BIGINT
);

CREATE TABLE "ROOM" (
    "ID" BIGINT NOT NULL PRIMARY KEY GENERATED ALWAYS AS IDENTITY,
    "CAPACITY" INTEGER NOT NULL,
    "PRICE" DOUBLE NOT NULL
);